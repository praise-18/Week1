C:

#include <stdio.h>
#include <string.h>
int main() {
    char str[100];
    scanf("%s", str);
    int len = strlen(str), flag = 1;
    for (int i = 0; i < len/2; i++) {
        if (str[i] != str[len-1-i]) {
            flag = 0;
            break;
        }
    }
    printf(flag ? "Palindrome\n" : "Not Palindrome\n");
    return 0;
}
Python:

s = input()
print("Palindrome" if s == s[::-1] else "Not Palindrome")

