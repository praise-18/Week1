C:
#include <stdio.h>
int main() {
    int arr[100], n, last;
    scanf("%d", &n);
    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);
    last = arr[n-1];
    for (int i = n-1; i > 0; i--)
        arr[i] = arr[i-1];
    arr[0] = last;
    for (int i = 0; i < n; i++)
        printf("%d ", arr[i]);
    return 0;
}
Python:
arr = list(map(int, input().split()))
arr = [arr[-1]] + arr[:-1]
print("Rotated array:", arr)
